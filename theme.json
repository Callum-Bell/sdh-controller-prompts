{
	"name": "Controller Prompts Galore",
	"version": "v1.0",
	"author": "ZeusOfTheCrows",
	"target": "System-Wide",
	"description": "description todo",
	"manifest_version": 3,
	"inject": {
		"global.css": [
			"SP"
		]
	},
	"patches": {
