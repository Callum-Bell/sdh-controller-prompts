{
	"name": "Controller Prompts Galore",
	"version": "v1.0",
	"author": "ZeusOfTheCrows",
	"target": "Tweak",
	"description": "change controller button prompts. includes dualshock 3 & SNES",
	"manifest_version": 3,
	"inject": {
		"global.css": [
			"SP", "QuickAccess"
		]
	},
	"patches": {
		"controller Type": {
			"default": "SNES",
			"type": "dropdown",
			"values":{
				"Super NES": {
					"snes/snes.css": [
						"SP", "QuickAccess"
					]
				},
				"Switch": {
					"nsx/nsx.css": [
						"SP", "QuickAccess"
					]
				},
				"Dualshock 3": {
					"ds3/ds3.css": [
						"SP", "QuickAccess"
					]
				}
			}
		}
	}
}