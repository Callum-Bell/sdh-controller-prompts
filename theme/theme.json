{
	"name": "Button Prompts Galore",
	"version": "v1.0",
	"author": "ZeusOfTheCrows",
	"target": "Tweak",
	"description": "change controller button prompts. includes snes, switch, dualshock 3 & 5, & xbox coloured",
	"manifest_version": 3,
	"inject": {
		"global.css": ["SP", "QuickAccess"]
	},
	"patches": {
		"controller Type": {
			"default": "Super NES",
			"type": "dropdown",
			"values":{
				"Super NES": {
					"snes/snes.css": ["SP", "QuickAccess"]
				},
				"Switch": {
					"nsx/nsx.css": ["SP", "QuickAccess"]
				},
				"DualShock": {
					"ds3/ds3.css": ["SP", "QuickAccess"]
				},
				"DualSense": {
					"ds5/ds5.css": ["SP", "QuickAccess"]
				},
				"XInput": {
					"xbox/xbox.css": ["SP", "QuickAccess"]
				}
			}
		}
	}
}
